{
  "name": "escolalms/video",
  "description": "Escola Headless LMS Video Processing",
  "type": "package",
  "require": {
    "php": ">=7.4 | >=8.0",
    "escolalms/courses": "^0",
    "escolalms/topic-types": "^0",
    "laravel/framework": ">=8.0",
    "league/flysystem-aws-s3-v3": "^1.0",
    "pbmedia/laravel-ffmpeg": "^7"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "orchestra/testbench": ">=5.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Mateusz Wojczal",
      "email": "mateusz@wojczal.com"
    },
    {
      "name": "Paweł Chołaj",
      "email": "pawel@cholaj.pl"
    }
  ],
  "autoload": {
    "psr-4": {
      "EscolaLms\\Video\\": "src",
      "EscolaLms\\Video\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "EscolaLms\\Video\\EscolaLmsVideoServiceProvider"
      ]
    }
  },
  "config": {
    "optimize-autoloader": false,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
